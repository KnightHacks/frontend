{
  "private": true,
  "scripts": {
    "with-env": "dotenv -e .env --",
    "build": "pnpm with-env turbo run build",
    "dev": "pnpm with-env turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "pnpm with-env turbo run db:push",
    "db:preview": "pnpm with-env turbo run db:preview",
    "db:migrate": "pnpm with-env turbo run db:migrate",
    "prepare": "husky install"
  },
  "prettier": "@vercel/style-guide/prettier",
  "devDependencies": {
    "@knighthacks/tsconfig": "workspace:*",
    "@vercel/style-guide": "^5.1.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "latest"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.9.0",
  "name": "knighthacks",
  "dependencies": {
    "dotenv-cli": "^7.3.0"
  }
}
