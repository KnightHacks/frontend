{
  "name": "@knighthacks/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.tsx",
      "./src/*.ts"
    ]
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ui-add": "pnpm dlx shadcn-ui add && prettier src --write --list-different"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@trpc/server": "11.0.0-rc.351",
    "class-variance-authority": "^0.7.0",
    "cmdk": "^0.2.0",
    "date-fns": "^3.5.0",
    "next-themes": "^0.2.1",
    "react-day-picker": "^8.10.0",
    "react-hook-form": "^7.50.1",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@knighthacks/eslint-config": "workspace:*",
    "@knighthacks/prettier-config": "workspace:*",
    "@knighthacks/tailwind-config": "workspace:*",
    "@knighthacks/tsconfig": "workspace:*",
    "@types/react": "^18.2.57",
    "eslint": "^9.4.0",
    "prettier": "^3.3.1",
    "react": "18.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "zod": "^3.23.8"
  },
  "prettier": "@knighthacks/prettier-config"
}
